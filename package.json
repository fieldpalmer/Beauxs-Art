    "create-react-app": "^3.3.0",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "formidable": "^1.2.1",
    "gridfs-stream": "^1.1.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongodb": "^3.2.7",
    "mongoose": "^5.5.9",
    "mongoose-gridfs": "^1.2.2",
    "morgan": "~1.9.0",
    "multer": "^1.4.1",
    "multer-gridfs-storage": "^3.2.3",
    "nodemon": "^1.19.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
